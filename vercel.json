{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/src/index.ts"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "OPTIONS",
                "DELETE"
            ]
        }
    ]
}